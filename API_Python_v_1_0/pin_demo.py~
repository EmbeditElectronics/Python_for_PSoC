from rpisoc import *

RPiSoC('SPI')

My_outputs = [DigitalOutput(4,0), DigitalOutput(4,1), DigitalOutput(5,0), DigitalOutput(5,1)]

try:
    while True:
        for i in My_outputs:
            i.Write(1)
            time.sleep(1)
        for i in reversed(My_outputs):
            i.Write(0)
            time.sleep(1)

except KeyboardInterrupt:
    RPiSoC.commChannel.close()
